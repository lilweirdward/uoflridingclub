    <script type="text/javascript">
        $('.faq > h2').click(function () {
            var id = $(this).attr('id');

            id = id.replace(/q/, "p");
            
            if ($('#' + id).is(':visible')) {
                $('#' + id).slideUp();
            }
            else {
                $('#' + id).slideDown();
            }
        });
